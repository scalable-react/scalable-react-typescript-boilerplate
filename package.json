{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "A simple ts react boilerplate",
  "main": "index.js",
  "author": "RyanCCollins",
  "license": "MIT",
  "engines": {
    "node": "6.2.0",
    "npm": "3.8.9"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "node devServer.js",
    "predeploy": "rimraf build/public/*.*",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "postdeploy": "npm run compile",
    "compile": "./node_modules/.bin/tsc",
    "setup:yarn": "yarn",
    "preserve": "npm run compile",
    "serve": "cross-env NODE_ENV=server PORT=1337 node server.js",
    "postinstall": "npm run deploy",
    "generator": "plop --plopfile ./config/generators/cli.js"
  },
  "dependencies": {
    "@types/body-parser": "latest",
    "@types/compression": "latest",
    "@types/express": "latest",
    "@types/graphql": "latest",
    "@types/isomorphic-fetch": "0.0.30",
    "@types/mongoose": "^4.7.5",
    "@types/morgan": "latest",
    "@types/node": "^6.0.54",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/reselect": "^2.0.27",
    "apollo-client": "0.8.5",
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "github-markdown-css": "^2.4.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.2.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "mongoose": "^4.8.2",
    "morgan": "^1.7.0",
    "node-env-file": "^0.1.8",
    "plop": "^1.7.3",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-apollo": "^0.10.1",
    "react-dom": "^15.4.1",
    "react-ga": "^2.1.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-markdown": "^2.4.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-connect": "^5.0.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "styled-components": "^1.2.1",
    "ts-loader": "^1.3.3",
    "tslint": "4.3.1",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.2.0",
    "typescript": "2.1.5",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
